.page-header
	h2!= message
div
	form(action='/import',method='POST', enctype='multipart/form-data')
		p User CSV file:
		p
			input(type='file', id='userFileImport', title='Choose a file', name='userFileImport', accept='text/comma-separated-values, text/csv, application/csv')
		p Food CSV file:
		p
			input(type='file', id='foodFileImport', title='Choose a file', name='foodFileImport', accept='text/comma-separated-values, text/csv, application/csv')
		p
			input(type='submit',value='Import', class="goButton")
